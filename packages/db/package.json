{
  "name": "@repo/db",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "prisma generate --schema ./src/schema.prisma",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --schema ./src/schema.prisma",
    "postinstall": "prisma generate --schema ./src/schema.prisma"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "zod-prisma-types": "^3.1.8"
  },
  "devDependencies": {
    "prisma": "^5.21.1"
  }
}
